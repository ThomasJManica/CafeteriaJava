	<queryString language="SQL">
		<![CDATA[SELECT pedidos.id,
	pedidos.quantidade,
	produtos.descricao,
	produtos.preco * pedidos.quantidade AS preco,
	pedidos.data_do_pedido
FROM pedidos,
	produtos
WHERE 
	 pedidos.id_produto = produtos.id 
	 AND pedidos.id_usuario = $P{id_usuario_parametro} 
