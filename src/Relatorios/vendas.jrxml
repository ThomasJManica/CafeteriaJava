	<queryString language="SQL">
		<![CDATA[SELECT usuarios.id AS id,
	usuarios.nome,
	count(*) AS quantidade
FROM pedidos,
	usuarios,
	produtos
WHERE 
	 pedidos.id_usuario = usuarios.id 
	 AND pedidos.id_produto = produtos.id 
